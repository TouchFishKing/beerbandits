<%= 'search#preferences' %>
<div class="search-bar">
        <%= form_tag results_path, method: :get do %>
          <%= text_field_tag :query,
            params[:query],
            class: "form-control",
            id: "surfboard_location",
            placeholder: "SEARCH BY LOCATION"
          %>
          <div id="banner-button">
            <%= submit_tag "Go", class: "button" %>
          </div>
        <% end %>
      </div>